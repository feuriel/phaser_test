    "compilerOptions": {
        "jsx": "react-jsx",
        "types": ["react", "react-dom"],
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/.pnpm/@types+*/node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "react": ["./node_modules/react"],
            "react-dom": ["./node_modules/react-dom"]
        }
    }
}

